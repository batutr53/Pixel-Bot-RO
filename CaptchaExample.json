{
  "profiles": {
    "CaptchaTest": {
      "global": {
        "captureMode": "PrintWindow",
        "clickMode": "message",
        "defaultHz": 60,
        "logLevel": "Info",
        "enableTelemetry": true,
        "dryRun": false
      },
      "windows": [
        {
          "titleRegex": "Game Client",
          "hwndString": null,
          "processName": "gameprocess.exe",
          "dpiAware": true,
          "captchaConfig": {
            "enabled": true,
            "captchaArea": {
              "X": 400,
              "Y": 350,
              "Width": 300,
              "Height": 100
            },
            "textBoxLocation": {
              "X": 700,
              "Y": 460
            },
            "submitButtonLocation": {
              "X": 700,
              "Y": 490
            },
            "detectionIntervalMs": 5000,
            "solveTimeoutMs": 30000,
            "processingOptions": {
              "processingMode": "Enhanced",
              "psmMode": "SingleWord",
              "useGrayscale": true,
              "useHistogramEqualization": true,
              "contrastFactor": 3.5,
              "sharpnessFactor": 3.0,
              "brightnessFactor": 1.3,
              "scaleFactor": 4
            },
            "templates": [
              {
                "name": "DefaultCaptcha",
                "searchArea": {
                  "X": 300,
                  "Y": 300,
                  "Width": 500,
                  "Height": 200
                },
                "detectionColors": [
                  {"R": 255, "G": 255, "B": 255},
                  {"R": 0, "G": 0, "B": 0}
                ],
                "colorTolerance": 30,
                "expectedPattern": "\\w{4,8}"
              }
            ]
          },
          "probes": [
            {
              "name": "HP",
              "kind": "point",
              "x": 50,
              "y": 50,
              "width": null,
              "height": null,
              "box": 5,
              "mode": "edge",
              "metric": "rgb",
              "refColor": [255, 0, 0],
              "toColor": null,
              "tolerance": 70,
              "debounceMs": 100
            }
          ],
          "events": [
            {
              "when": "HP",
              "click": {
                "x": 100,
                "y": 100,
                "useCoordinate": false,
                "useKeyPress": true,
                "keyToPress": "1"
              },
              "cooldownMs": 1000,
              "priority": 1
            }
          ]
        }
      ]
    }
  },
  "hotkeySets": {
    "Default": {
      "toggleOverlay": "Ctrl+~",
      "lockWindow": "F2",
      "toggleSnap": "F3",
      "magnifierToggle": "F4",
      "captureModeCycle": "F6",
      "clickModeCycle": "F7",
      "shapePoint": "P",
      "shapeRect": "R",
      "deleteShape": "Del",
      "copyColorAtCursor": "Ctrl+Shift+C",
      "saveConfig": "Ctrl+S",
      "openConfig": "Ctrl+O",
      "profileNext": "Ctrl+Alt+Right",
      "profilePrev": "Ctrl+Alt+Left",
      "panicStopAll": "Pause"
    }
  },
  "activeHotkeys": "Default"
}