<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- BabeMakro Logo Path Data -->
    <DrawingImage x:Key="BabeMakroLogo">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Background Circle -->
                <GeometryDrawing Brush="#FF1493">
                    <GeometryDrawing.Geometry>
                        <EllipseGeometry Center="32,32" RadiusX="30" RadiusY="30"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Gear/Settings Symbol -->
                <GeometryDrawing Brush="#FFD700">
                    <GeometryDrawing.Geometry>
                        <PathGeometry Figures="M32,16 L36,20 L40,16 L42,18 L42,22 L40,24 L36,28 L32,24 L28,28 L24,24 L22,22 L22,18 L24,16 L28,20 Z"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                
                <!-- Text "BM" -->
                <GeometryDrawing Brush="#FFFFFF">
                    <GeometryDrawing.Geometry>
                        <PathGeometry Figures="M20,38 L20,48 L24,48 L24,44 L26,44 L26,48 L30,48 L30,38 L26,38 L26,42 L24,42 L24,38 Z M34,38 L34,48 L38,48 L38,44 L40,48 L44,48 L44,38 L40,38 L38,42 L36,38 Z"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>
    
    <!-- Application Colors -->
    <SolidColorBrush x:Key="BabeMakroPrimary" Color="#FF1493"/>
    <SolidColorBrush x:Key="BabeMakroSecondary" Color="#FFD700"/>
    <SolidColorBrush x:Key="BabeMakroAccent" Color="#FF69B4"/>
    
</ResourceDictionary>